{
  "globals": {
    "Mustache": true,
    "$": true
  },
  "extends": [
	  "eslint:recommended",
  	"plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
	    	"jsx": true
	 },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "jquery": true,
    "node": true,
    "es2021": true
  },
  "plugins":[
	  	"react"
	 ],
  "rules": {
	  "react/react-in-jsx-scope": "off",
	  "react/forbid-prop-types": [0, { "forbid": ["any"] }],
    "react/prop-types": 0,
    "eol-last": "error",
    "eqeqeq": [
      "error",
      "always"
    ],
    "indent": [
      "error",
      2
    ],
    "new-cap": "warn",
    "no-console": "off",
    "no-multi-spaces": [
      "warn",
      {
        "exceptions": {
          "VariableDeclarator": true
        }
      }
    ],
    "no-redeclare": [
      "error",
      {
        "builtinGlobals": true
      }
    ],
    "no-template-curly-in-string": "error",
    "no-trailing-spaces": "warn",
    "no-undefined": "off",
    "quotes": [
      "warn",
      "single",
      {
        "allowTemplateLiterals": true
      }
    ],
    "semi": "error"
  }
}